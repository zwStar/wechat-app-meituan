.star {
  width: 350rpx;
  display: inline-block;
  font-size: 0;
}

.on, .half, .off {
  display: inline-block;
  width: 45rpx;
  height: 45rpx;
  margin-right: 5rpx;
  background-size: 100%;
  background-repeat: no-repeat;
}

/* 背景不支持url，这里需要用base64编码 */

.on {
  background-image: url("data:image/png;base64,");
}

.half {
  background-image: url("data:image/png;base64,");
}

.off {
  background-image: url("data:image/png;base64,");
}
